def age_calculator():
    from datetime import datetime
    current_date = datetime.now().year
    user_input = int(input("Your input here"))
    years = current_date - user_input
    if user_input > current_date:
        print ("Unsupported")
    else:
        print(f"you are {years} old")
        print(f"You will be 50 in {user_input + 50}")
